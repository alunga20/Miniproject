npts 40 40 40                        # num.grid points in xyz
gridfld ligand_pino.maps.fld         # grid_data_file
spacing 0.375                        # spacing(A)
receptor_types A C HD OA             # receptor atom types
ligand_types A C HD N NA OA SA       # ligand atom types
receptor ligand_pino.pdbqt           # macromolecule
gridcenter auto                      # xyz-coordinates or auto
smooth 0.5                           # store minimum energy w/in rad(A)
map ligand_pino.A.map                # atom-specific affinity map
map ligand_pino.C.map                # atom-specific affinity map
map ligand_pino.HD.map               # atom-specific affinity map
map ligand_pino.N.map                # atom-specific affinity map
map ligand_pino.NA.map               # atom-specific affinity map
map ligand_pino.OA.map               # atom-specific affinity map
map ligand_pino.SA.map               # atom-specific affinity map
elecmap ligand_pino.e.map            # electrostatic potential map
dsolvmap ligand_pino.d.map              # desolvation potential map
dielectric -0.1465                   # <0, AD4 distance-dep.diel;>0, constant
